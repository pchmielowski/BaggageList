import java.lang.Boolean;
import net.chmielowski.baggage.ui.EquipmentId;

CREATE TABLE equipment (
    id INTEGER AS EquipmentId PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    isPacked INTEGER AS Boolean NOT NULL
);

selectEquipments:
SELECT *
FROM equipment
;

insertEquimpent:
INSERT INTO equipment (name, isPacked)
VALUES (?, 0)
;

setEquipmentPacked:
UPDATE equipment
SET isPacked = ?2
WHERE id = ?1
;
